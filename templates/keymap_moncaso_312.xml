      <!-- VolumeDown --> <obc010>VolumeDown</obc010>
      <!-- ChannelUp --> <obc011>PageUp</obc011>
      <!-- ChannelDown --> <obc012>PageDown</obc012>
      <!-- Red --> <obc013>ActivateWindow(Videos,Movies)</obc013>
      <!-- Green --> <obc016>ActivateWindow(Videos,TvShows)</obc016>
      <!-- Yellow --> <obc015>ActivateWindow(Music)</obc015>
      <!-- Blue --> <obc014>PlayPvrTV</obc014>
  
      <!-- Ok --> <obc017>Select</obc017>
      <!-- Left --> <obc018>Left</obc018>
      <!-- Up --> <obc019>Up</obc019>
      <!-- Down --> <obc020>Down</obc020>
      <!-- Right --> <obc021>Right</obc021>
      <!-- Open --> <obc022>ContextMenu</obc022>
      <!-- Back --> <obc024>Back</obc024>
      <!-- Exit --> <obc025>Close</obc025>
      <!-- Info --> <obc026>Info</obc026>
      <!-- Home --> <obc027>ActivateWindow(Home)</obc027>
      <!-- Power --> <obc028>Shutdown()</obc028>
  
      <!-- 2 --> <obc031>JumpSMS2</obc031>
      <!-- 3 --> <obc032>JumpSMS3</obc032>
      <!-- 4 --> <obc033>JumpSMS4</obc033>
      <!-- 5 --> <obc034>JumpSMS5</obc034>
