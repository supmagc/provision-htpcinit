<keymap>
  <!-- Default actions -->
  <global>
    <universalremote>
      <!-- Play --> <obc001>PlayPause</obc001>
      <!-- Pause --> <obc002>Pause</obc002>
      <!-- Stop --> <obc003>Stop</obc003>
      <!-- Next --> <obc004>SkipNext</obc004>
      <!-- Previous --> <obc005>SkipPrevious</obc005>
      <!-- Forward --> <obc006>FastForward</obc006>
      <!-- Reverse --> <obc007>Rewind</obc007>

      <!-- Mute --> <obc008>Mute</obc008>
